{
  "name": "@stash-it/mysql-adapter",
  "version": "0.5.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": "./mod.ts",
  "files": [
    "mod.ts",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "_dist",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "keywords": [
    "@stash-it",
    "stash it",
    "stash-it",
    "adapter",
    "mysql",
    "MySQL"
  ],
  "scripts": {
    "lint": "biome check .",
    "test": "./bin/when-mysql-is-ready-run.sh vitest run",
    "test:watch": "./bin/when-mysql-is-ready-run.sh vitest --watch",
    "typecheck": "tsc --noEmit",
    "release": "pnpm -w run check-versions && pnpx jsr publish"
  },
  "dependencies": {
    "mysql2": "^3.16.2",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@stash-it/core": ">=0.5.0"
  },
  "devDependencies": {
    "@stash-it/core": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@stash-it/dev-tools": "workspace:*",
    "dotenv": "catalog:",
    "vitest": "catalog:"
  }
}
