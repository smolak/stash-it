    "publish": "pnpm -w run check-versions && pnpx jsr publish"
  },
  "peerDependencies": {
    "@stash-it/core": ">=0.5.0"
  },
  "devDependencies": {
    "@stash-it/core": "workspace:*",
    "@biomejs/biome": "catalog:",
    "@stash-it/dev-tools": "workspace:*",
    "@stash-it/memory-adapter": "workspace:*",
    "@stash-it/stash-it": "workspace:*",
    "vitest": "catalog:"
  }
}
