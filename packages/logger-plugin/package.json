{
  "name": "@stash-it/logger-plugin",
  "version": "0.5.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": "./mod.ts",
  "files": [
    "mod.ts",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "_dist",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "keywords": [
    "@stash-it",
    "stash it",
    "stash-it",
    "plugin",
    "logger",
    "logging"
  ],
  "scripts": {
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
