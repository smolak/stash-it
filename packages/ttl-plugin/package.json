{
  "name": "@stash-it/ttl-plugin",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": "./mod.ts",
  "files": [
    "mod.ts",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "_dist",
    "README.md",
    "package.json"
  ],
  "license": "MIT",
  "keywords": [
    "@stash-it",
    "stash it",
    "stash-it",
    "plugin",
    "ttl"
  ],
  "scripts": {
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "publish": "pnpx jsr publish --allow-slow-types"
  },
  "dependencies": {
    "@stash-it/core": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@stash-it/dev-tools": "workspace:*",
    "@stash-it/memory-adapter": "workspace:*",
    "@stash-it/stash-it": "workspace:*",
    "vitest": "catalog:"
  }
}
